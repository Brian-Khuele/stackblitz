    "@types/express": "^4.17.6",
    "@types/find": "^0.2.1",
    "@types/helmet": "0.0.45",
    "@types/jasmine": "^3.5.10",
    "@types/jsonfile": "^5.0.0",
    "@types/mailgen": "^2.0.3",
    "@types/morgan": "^1.9.0",
    "@types/multer": "^1.4.4",
    "@types/node": "^13.13.42",
    "@types/nodemailer": "^6.4.0",
    "@types/supertest": "^2.0.8",
    "@types/uuid": "^8.3.0",
    "find": "^0.3.0",
    "fs-extra": "^9.0.0",
    "jasmine": "^3.5.0",
    "jsonfile": "^6.0.1",
    "nodemon": "^2.0.3",
    "supertest": "^4.0.2",
    "ts-node": "^8.8.2",
