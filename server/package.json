{
  "name": "hydra",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "node ./util/build.js",
    "lint": "tslint --project \"tsconfig.json\"",
    "start": "node -r module-alias/register ./dist",
    "start:dev": "nodemon --config nodemon.json",
    "test": "nodemon --config nodemon.test.json",
    "clean": "concurrently  \"knex migrate:rollback\"  \"knex migrate:latest\"  \"knex seed:run\""
  },
  "_moduleAliases": {
    "@daos": "dist/daos",
    "@entities": "dist/entities",
    "@shared": "dist/shared",
    "@server": "dist/Server",
    "@config": "config"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "command-line-args": "^5.1.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "helmet": "^3.22.0",
    "http-status-codes": "^1.4.0",
    "knex": "^0.21.12",
    "knex-migrate": "^1.7.4",
    "mailgen": "^2.0.14",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "nodemailer": "^6.4.18",
    "oracledb": "^4.2.0",
    "pg": "^8.1.0",
    "uuid": "^8.3.1",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/command-line-args": "^5.0.0",
    "@types/cookie-parser": "^1.4.2",
